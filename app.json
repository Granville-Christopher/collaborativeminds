      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#5865F2"
      }
    },
    "plugins": [
      "expo-web-browser"
    ],
    "scheme": "collaborativesuccess",
    "extra": {
      "eas": {
        "projectId": "e0e7a53a-f590-481b-9cb5-ed9fc4fd945a"
      }
    }
